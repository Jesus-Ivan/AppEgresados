<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.pruebarecyclerview">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />


    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_app_square"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_app_circle"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.NoActionBar">
        <activity android:name=".activities.NuevaPublicacion"></activity>
        <activity android:name=".activities.MainActivity" />
        <activity android:name=".activities.PubPendientes" />
        <activity android:name=".activities.ControlPub" />
        <activity android:name=".activities.MyProfile" />
        <activity android:name=".activities.Registro" />
        <activity android:name=".activities.InterfazPrinc" />
        <activity android:name=".activities.MainActivity2">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>